{
  "plugins": ["prettier"],
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "prettier/prettier": "error",
    "import/extensions": "off",
    "import/no-unresolved": 0,
    "no-use-before-define": 0,
    "import/no-extraneous-dependencies": 0,
    "no-shadow": 0,
    "no-console": "off"
  }
}
